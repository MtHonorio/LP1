//Codigo do Maior Numero
#include <iostream>

using namespace std;
int main (){
    int y, x, maior;

    while (1){

    cout << "informe dois numero: ";
    cin >> y;
    cin >> x;

    if (x == 0 || y == 0){
        break;
    }
    if (x > y){
        maior = x;
    }else{
        maior = y;
    }
    cout << "O numero maior eh: " << maior << endl;
    }
    return 0;
}
